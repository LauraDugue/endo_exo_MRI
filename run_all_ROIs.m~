%% run_all_ROIs
%%% mr: scanNum=1 for exo and scanNum=2 for endo
%%% id: scanNum=1 for exo and scanNum=2 for endo

scanNum = 1:2
hemi = {'l', 'r'};
roiNames = {'v1', 'v2d', 'v3d', 'v3a', 'v3b', 'v4', 'v7', 'lo1', 'lo2', 'vo1', 'vo2'};
v = newView;


for iScan = 1:length(scanNum);
    for iHemi=1:length(hemi)
        for iRoi=1:length(roiNames)
            v = endoexoRvL(v, sprintf('%s_%s', hemi{iHemi}, roiNames{iRoi}), sprintf('scanNum=%i', scan 
            
            
            'r_v1','exo','scanNum=1');
        

%% Right exo
v = endoexoRvL(v, 'r_v1','exo','scanNum=1');
